{
  "name": "TrainerRoad Workouts To Zwift",
  "version": "0.3.0",
  "manifest_version": 3,
  "permissions": [],
  "host_permissions": ["https://www.trainerroad.com/"],
  "content_scripts": [
    {
      "matches": ["https://*.trainerroad.com/*"],
      "js": ["buttonScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "tr_zwift_logo.png"
    }
  },
  "icons": {
    "128": "tr_zwift_logo.png"
  }
}
